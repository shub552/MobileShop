			<div class="contentbg">
                <h1>Product List</h1>
                <span><h3 style="color: crimson;"> <%= message %> 
                <%= msg %>     <!-- message or msg me se ek bar me ek he chalega, isliye yahi likh diya, varna alag alag span me bhi likh sakte he. -->
                </h3></span>
				<div class="post" style="margin-left:-350px;">
					<div class="entry">
                        <table id="myTable" style="width:100%">
                            <thead>
                                <tr>
                                    <th>Product Id</th>
                                    <th>Category Name</th>
                                    <th>Product Name</th>
                                    <th>Price</th>
                                    <th>Quantity</th>
                                    <th>Image</th>
                                    <th>Update</th>
                                    <th>Delete</th>
                                  </tr>
                            </thead>
                            <tbody>
                                <%for(index in products){%>
                                <tr align="center">
                                    <td><%=products[index]._id%></td>
                                    <td><%=products[index].cname%></td>
                                    <td><%=products[index].pname%></td>
                                    <td><%=products[index].price%></td>
                                    <td><%=products[index].quantity%></td>
                                    <td><img height='80' width='80' src='/images/<%=products[index].filename%>' alt='Image'></td>
                                    <td><a href='/admin/update-product/<%=products[index]._id%>'>Update</a></td>
                                    <td><a href='/admin/delete-product/<%=products[index]._id%>'>Delete</a></td>
                                </tr>
                                <%}%>
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th>Product Id</th>
                                        <th>Category Name</th>
                                        <th>Product Name</th>
                                        <th>Price</th>
                                        <th>Quantity</th>
                                        <th>Image</th>
                                        <th>Update</th>
                                        <th>Delete</th>
                                      </tr>
                                    </tfoot>
                               </table>					</div>
